{
  "extends": "tslint:latest",
  "rules": {
    "object-literal-sort-keys": false,
    "no-reference": false,
    "no-console": [
      true,
      "log",
      "debug"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ]
  }
}